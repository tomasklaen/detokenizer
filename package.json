{
	"name": "detokenizer",
	"version": "1.0.1",
	"description": "Replace tokens in a string.",
	"author": "tomasklaen",
	"repository": "https://github.com/tomasklaen/detokenizer",
	"license": "MIT",
	"keywords": [
		"string",
		"replace",
		"token"
	],
	"files": [
		"dist"
	],
	"main": "./dist/main.js",
	"types": "./dist/main.d.ts",
	"scripts": {
		"format": "prettier --write \"src/**/*.ts\"",
		"test": "prettier --check \"src/**/*.ts\" & jest",
		"clean": "rm -rf dist",
		"build": "npm run clean & tsc -p tsconfig.build.json",
		"start": "npm run clean & jest --watchAll",
		"prepack": "npm run build"
	},
	"devDependencies": {
		"@types/jest": "^26.0.21",
		"@types/node": "^15.0.1",
		"jest": "^26.6.3",
		"prettier": "^2.2.1",
		"ts-jest": "^26.5.4",
		"typescript": "^4.2.3"
	}
}
